<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
    <class name="cz.cuni.mff.ufal.textan.data.tables.InRelationTable" table="IsInRelation">
      <meta attribute="class-description">
         Join table between relation and object.
      </meta>
      <id 
          name="id" 
          type="long" 
          column="id_is_in_relation"
          unsaved-value="0">
         <generator class="native" />
      </id>  
       
       <property name="order" column="order_in_relation" type="integer"/>
       
       <many-to-one name="relation" class="cz.cuni.mff.ufal.textan.data.tables.RelationTable" 
                    column="id_relation" not-null="true" lazy="false" cascade="save-update" />
       <many-to-one name="object" class="cz.cuni.mff.ufal.textan.data.tables.ObjectTable" 
                    column="id_object" not-null="true" lazy="false" cascade="save-update"/>
    </class>
    
</hibernate-mapping>
