<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
    <class name="cz.cuni.mff.ufal.textan.data.tables.RelationTable" table="Relation">
      <meta attribute="class-description">
         Relation itself 
      </meta>
      <id 
          name="id" 
          type="long" 
          column="id_relation"
          unsaved-value="0">
         <generator class="native" />
      </id>  
       <many-to-one name="relationType" class="cz.cuni.mff.ufal.textan.data.tables.RelationTypeTable" 
                    column="id_relation_type" not-null="true" lazy="false" />
    </class>
    
</hibernate-mapping>
